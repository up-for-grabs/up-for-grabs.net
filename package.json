    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.6",
    "jest-fetch-mock": "^3.0.3",
    "jest-localstorage-mock": "^2.4.14",
    "prettier": "^2.3.2",
    "promise-polyfill": "^8.2.0",
