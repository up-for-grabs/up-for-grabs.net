{
  "plugins": ["import", "jest"],
  "extends": ["airbnb-base", "prettier", "plugin:jest/recommended"],
  "rules": {
    "no-undef": "off",
    "vars-on-top": "off",
    "no-var": "off",
    "func-names": "off",
    "one-var": "off",
    "eqeqeq": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "no-useless-escape": "off",
    "no-restricted-globals": "off",
    "no-use-before-define": "off",
    "global-require": "off",
    "import/no-amd": "off"
  }
}
