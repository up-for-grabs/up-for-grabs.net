    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-localstorage-mock": "^2.4.0",
    "prettier": "^1.19.1",
    "promise-polyfill": "^8.1.3",
    "typescript": "^3.7.5",
    "underscore": "^1.9.2",
    "whatwg-fetch": "^3.0.0"
